{
  "routes": [
    {
      "route": "/health",
      "headers": {
        "Content-Type": "application/json",
        "Content-Disposition": "inline"
      }
    }
  ],
  "mimeTypes": {
    ".json": "application/json"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/health"]
  }
}
